@model ASPNETMVCCRUD.Models.AddEmployeeViewModel
@{
}
<h1>Add Employee</h1>
<form method="post" action="Add">
  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Name</label>
    <input type="text" class="form-control" id="exampleInputEmail1" asp-for="Name">
  </div>
  <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Email address</label>
    <input type="email" class="form-control" id="exampleInputEmail1" asp-for="Email">
  </div>
   <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Salary</label>
    <input type="number" class="form-control" id="exampleInputEmail1" asp-for="Salary">
  </div>
   <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Date Of Birth</label>
    <input type="date" class="form-control" id="exampleInputEmail1" asp-for="DateOfBirth">
  </div>
    <div class="mb-3">
    <label for="exampleInputEmail1" class="form-label">Department</label>
    <input type="text" class="form-control" id="exampleInputEmail1" asp-for="Department">
  </div>
  
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
